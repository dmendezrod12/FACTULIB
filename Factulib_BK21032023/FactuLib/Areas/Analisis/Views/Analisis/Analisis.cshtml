@{
    ViewData["Title"] = "Analisis";
}

<div class="row">
    <div class="col-sm-6">
        <div class="card">
            <div class="card-header">
                Bar Chart
            </div>
            <div class="card-body">
                <canvas id="myBarChart" width="100" height="50">
                </canvas>
            </div>
        </div>
    </div>
    <div class="col-sm-6">
        <div class="card">
            <div class="card-header">
                Pie Chart
            </div>
            <div class="card-body">
                <canvas id="myPieChart" width="100" height="50">
                </canvas>
            </div>
        </div>
    </div>
</div>

